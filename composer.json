{
    "name": "ricardoboss/php-seq",
    "description": "Implements a PHP client for the Seq (by Datalust) HTTP ingestion API",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "type": "library",
    "require": {
        "php": "^8.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "guzzlehttp/guzzle": "^7.6"
    },
    "autoload": {
        "psr-4": {
            "Ricardoboss\\PhpSeq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ricardoboss\\PhpSeq\\": "test/"
        }
    }
}

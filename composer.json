{
    "name": "ricardoboss/php-seq",
    "description": "Implements a PHP client for the Seq (by Datalust) HTTP ingestion API",
    "license": "MIT",
    "keywords": [
        "Seq",
        "Datalust",
        "logging"
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "type": "library",
    "require": {
        "php": "^8.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "@stable"
    },
    "autoload": {
        "psr-4": {
            "RicardoBoss\\PhpSeq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicardoBoss\\PhpSeq\\": "test/"
        }
    }
}
